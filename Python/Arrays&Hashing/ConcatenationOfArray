class Solution(object):
    def getConcatenation(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        # Solution 1
        '''
        return nums + nums
        '''
        # Solution 2
        '''
        return nums*2
        '''
        # Solution 3
        ans = []
        for i in range(2):
            for n in nums:
                ans.append(n)
        return ans
